<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
    xmlns="https://github.com/spotbugs/filter/4.8.5"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/4.8.5 https://raw.githubusercontent.com/spotbugs/spotbugs/4.8.5/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Class name="org.hyperledger.fabric.client.GatewayException" />
        <Method name="printStackTrace" />
        <Bug pattern="DM_DEFAULT_ENCODING" />
    </Match>
    <Match>
        <Class name="org.hyperledger.fabric.client.identity.X509Identity" />
        <Field name="certificate" />
        <Or>
            <Bug pattern="EI_EXPOSE_REP" />
            <Bug pattern="EI_EXPOSE_REP2" />
        </Or>
    </Match>

</FindBugsFilter>
